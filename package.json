{
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "prisma": "^6.19.0"
  },
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsup src",
    "vercel": "npm i && npx prisma generate && tsc && npx vercel",
