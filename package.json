    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "swagger": "ts-node src/swagger-output.ts",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
